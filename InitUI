-- Functions to check exploit functions and gui

function CheckExploitFunctions()
  if getreg and debug.setupvalue and debug.getupvalue and debug.getupvalues and getsenv and getrenv and require and setclipboard and getgc and debug.getregistry and writefile then
  else
    error("Your exploit DOES not support this script!")
  end
end

function CheckIfAlreadyExists()
  for i,v in pairs(game.CoreGui:GetDescendants()) do
    if v.ClassName == "ScreenGui" then
      if v.Name == "clvcore" then
        error("GUI already exists")
      end
    end
  end
end




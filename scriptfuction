function ScriptFunction()
for i,v in pairs(game.Players.LocalPlayer:GetDescendants()) do -- searches for localscripts if found then make a texbutton for them
		if v.ClassName == "LocalScript" then
			
			local Con = 0
			local Pro = 0
			
			
			
local ScriptInfo = Instance.new("Frame")
local ScriptIcon_xd = Instance.new("ImageLabel")
local ScriptName = Instance.new("TextLabel")
local ScriptInfoBtn = Instance.new("TextButton")
local ProtoText = Instance.new("TextLabel")
local ConstantText = Instance.new("TextLabel")

--Properties:

ScriptInfo.Name = "ScriptInfo"
ScriptInfo.Parent = ScriptContainer
ScriptInfo.BackgroundColor3 = Color3.fromRGB(58, 58, 58)
ScriptInfo.BorderSizePixel = 0
ScriptInfo.Size = UDim2.new(0, 395, 0, 36)

ScriptIcon_xd.Name = "ScriptIcon_xd"
ScriptIcon_xd.Parent = ScriptInfo
ScriptIcon_xd.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ScriptIcon_xd.BackgroundTransparency = 1.000
ScriptIcon_xd.Size = UDim2.new(0, 36, 0, 36)
ScriptIcon_xd.Image = "rbxassetid://4800245565"

ScriptName.Name = "ScriptName"
ScriptName.Parent = ScriptInfo
ScriptName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ScriptName.BackgroundTransparency = 1.000
ScriptName.Position = UDim2.new(0.106329113, 0, 0, 0)
ScriptName.Size = UDim2.new(0, 221, 0, 36)
ScriptName.Font = Enum.Font.SourceSans
ScriptName.Text = tostring(v.Name)
ScriptName.TextColor3 = Color3.fromRGB(255, 255, 255)
ScriptName.TextSize = 18.000
ScriptName.TextXAlignment = Enum.TextXAlignment.Left

ScriptInfoBtn.Name = "ScriptInfoBtn"
ScriptInfoBtn.Parent = ScriptInfo
ScriptInfoBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ScriptInfoBtn.BackgroundTransparency = 1.000
ScriptInfoBtn.Size = UDim2.new(0, 395, 0, 36)
ScriptInfoBtn.Visible = true
ScriptInfoBtn.Font = Enum.Font.SourceSans
			ScriptInfoBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
			ScriptInfoBtn.BackgroundTransparency = 1
			ScriptInfoBtn.Text = ""
ScriptInfoBtn.TextSize = 14.000

ProtoText.Name = "ProtoText"
ProtoText.Parent = ScriptInfo
ProtoText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ProtoText.BackgroundTransparency = 1.000
ProtoText.Position = UDim2.new(0.711392403, 0, 0.0555555895, 0)
ProtoText.Size = UDim2.new(0, 8, 0, 34)
ProtoText.Font = Enum.Font.SourceSans
ProtoText.Text = "0"
ProtoText.TextColor3 = Color3.fromRGB(255, 255, 255)
ProtoText.TextSize = 18.000
ProtoText.TextXAlignment = Enum.TextXAlignment.Left

ConstantText.Name = "ConstantText"
ConstantText.Parent = ScriptInfo
ConstantText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ConstantText.BackgroundTransparency = 1.000
ConstantText.Position = UDim2.new(0.903797448, 0, 0.0555555895, 0)
ConstantText.Size = UDim2.new(0, 8, 0, 34)
ConstantText.Font = Enum.Font.SourceSans
ConstantText.Text = "0"
ConstantText.TextColor3 = Color3.fromRGB(255, 255, 255)
ConstantText.TextSize = 18.000
			ConstantText.TextXAlignment = Enum.TextXAlignment.Left
			ScriptContainer.CanvasSize = ScriptContainer.CanvasSize + UDim2.new(0,0,0.2,0)
			
			
			
			
			RightScript.MouseLeave:connect(function()
				RightScript.Visible = false
				end)
			
			ScriptInfoBtn.MouseButton2Click:connect(function()
				pros = tonumber(ProtoText.Text)
				pcall(function()
				
					for i2,v2 in pairs(game.Players.LocalPlayer:GetDescendants()) do
		if v2.ClassName == "LocalScript" then
						if v2.Name == ScriptName.Text then
						
				
				getconstantss = debug.getconstants(getscriptclosure(v2))
			for i,v in pairs(getconstantss) do
   ConstantOutput(v)
							end
							end
					end
					
						end
				
				
			for i2,v2 in pairs(game.Players.LocalPlayer:GetDescendants()) do
		if v2.ClassName == "LocalScript" then
						if v2.Name == ScriptName.Text then
							
							for i3,v3 in pairs(getsenv(v2)) do -- function to get their functions/variables
	if type(v3) == 'function' then -- check if its a function
				
									FunctionOutput(i3,v3)
								end
							end
						end
					end
				end
				
				RightScript.Visible = true
					local plr = game.Players.LocalPlayer

local mouse = plr:GetMouse()

							
							
					RightScript.Position = UDim2.new(0, mouse.X, 0, mouse.Y)
					end)
			end)
			
			
			pcall(function() -- count protos/fuinctopns
			for i2,v2 in pairs(getsenv(v)) do
				if type(v2) == "function" then
					Pro = Pro +1
				end
				
				end
			
			
				ProtoText.Text = Pro
				end)
			
			local getcons = debug.getconstants(getscriptclosure(v))
			
			
		
			for i,v in pairs(getcons) do
				   -- print(i,v)
				Con = Con + 1
end


						
				
				
			
			
				ConstantText.Text = Con
				
			
			ViewConstantsBtn.MouseButton1Click:connect(function()
				
				ScriptContainer.CanvasSize = UDim2.new(0,0,0,0)
				RightScript.Visible = false
				ClearScriptCon()
				ScriptConstantContainer.Visible = Text
			
				Text_3.Visible = false
				Text_2.Visible = false
				Text.Visible = false
				
				end)
		
			ViewfuncsBtn.MouseButton1Click:connect(function()
				ScriptContainer.CanvasSize = UDim2.new(0,0,0,0)
				RightScript.Visible = false
				ClearScriptCon()
				ScriptFunctionContainer.Visible = true
			
				Text_3.Visible = false
				Text_2.Visible = false
				Text.Visible = false
				
				
				
							
				
									
									
									
							
							
							
					
				
			end)
			
			
			GetPathBtn.MouseButton1Click:connect(function()
				
				RightScript.Visible = false
				
				for i,v in pairs(game:GetDescendants()) do
					if v.ClassName == "LocalScript" then
						if v.Name == Scriptk then
							setclipboard("-- This script was generated with clvcore\n\ngame." .. v:GetFullName())
						end
					end
				end
				
				
			end)
			
			
			
		end
	end
	
	
	for i,v in pairs(game.Workspace:GetDescendants()) do -- searches for localscripts if found then make a texbutton for them
		if v.ClassName == "LocalScript" then
			
			local Con = 0
			local Pro = 0
			
			
			

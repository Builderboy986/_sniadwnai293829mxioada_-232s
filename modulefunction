          GenerateModuleScriptBtn.BackgroundColor3 = Color3.fromRGB(58, 58, 58)
          GenerateModuleScriptBtn.BorderSizePixel = 0
          GenerateModuleScriptBtn.Position = UDim2.new(0.0842391327, 0, 0.0161289517, 0)
          GenerateModuleScriptBtn.Size = UDim2.new(0, 333, 0, 25)
          GenerateModuleScriptBtn.Font = Enum.Font.Code
          GenerateModuleScriptBtn.Text = v.Name
          GenerateModuleScriptBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
          GenerateModuleScriptBtn.TextSize = 16.000
          GenerateModuleScriptBtn.TextXAlignment = Enum.TextXAlignment.Left

          TableMain.Name = "TableMain"
          TableMain.Parent = ModuleInfoMain
          TableMain.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
          TableMain.BorderSizePixel = 0
          TableMain.Position = UDim2.new(0.0108695654, 0, 0.336271137, 0)
          TableMain.Size = UDim2.new(0, 360, 0, 40)

          TableNameIcon.Name = "TableNameIcon"
          TableNameIcon.Parent = TableMain
          TableNameIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
          TableNameIcon.BackgroundTransparency = 1.000
          TableNameIcon.BorderSizePixel = 0
          TableNameIcon.Position = UDim2.new(0, 0, 0.0250000004, 0)
          TableNameIcon.Size = UDim2.new(0, 18, 0, 18)
          TableNameIcon.Image = "rbxassetid://4702833493"
          TableNameIcon.ImageColor3 = Color3.fromRGB(230, 230, 113)

          TableValueText.Name = "TableValueText"
          TableValueText.Parent = TableNameIcon
          TableValueText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
          TableValueText.BackgroundTransparency = 1.000
          TableValueText.BorderColor3 = Color3.fromRGB(27, 42, 53)
          TableValueText.Position = UDim2.new(1.5, 0, 0.222222224, 0)
          TableValueText.Size = UDim2.new(0, 333, 0, 10)
          TableValueText.Font = Enum.Font.SourceSans
         game:GetService("RunService").RenderStepped:Connect(function()
							          TableValueText.Text = tostring(i2) -- name
							end)
          TableValueText.TextColor3 = Color3.fromRGB(225, 150, 85)
          TableValueText.TextSize = 16.000
          TableValueText.TextXAlignment = Enum.TextXAlignment.Left

          ValueNameIcon.Name = "ValueNameIcon"
          ValueNameIcon.Parent = TableMain
          ValueNameIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
          ValueNameIcon.BackgroundTransparency = 1.000
          ValueNameIcon.BorderSizePixel = 0
          ValueNameIcon.Position = UDim2.new(-0.00833333377, 0, 0.475000024, 0)
          ValueNameIcon.Size = UDim2.new(0, 21, 0, 21)
          ValueNameIcon.Image = "rbxassetid://4702854135"
          ValueNameIcon.ImageColor3 = Color3.fromRGB(230, 230, 113)

          TableValueBox.Name = "TableValueBox"
          TableValueBox.Parent = ValueNameIcon
          TableValueBox.BackgroundColor3 = Color3.fromRGB(58, 58, 58)
          TableValueBox.BackgroundTransparency = 1.000
          TableValueBox.BorderSizePixel = 0
          TableValueBox.Position = UDim2.new(1.42857146, 0, 0.238095239, 0)
          TableValueBox.Size = UDim2.new(0, 329, 0, 10)
          TableValueBox.Font = Enum.Font.SourceSans
						          TableValueBox.PlaceholderText = "ValueHere"
						game:GetService("RunService").RenderStepped:Connect(function()
							          TableValueBox.Text = tostring(v2) -- index
							end)
          TableValueBox.TextColor3 = Color3.fromRGB(255, 255, 255)
          TableValueBox.TextSize = 14.000
          TableValueBox.TextXAlignment = Enum.TextXAlignment.Left
          ModuleValueContainer.CanvasSize = ModuleValueContainer.CanvasSize + UDim2.new(0,0,0.4,0)



          ClearModuleCon()
          wait()
          ModuleValueContainer.Visible = true





        end






        end) -- end of button func









      end


		    end
		
		
		
		

	  end
	
	
	
	

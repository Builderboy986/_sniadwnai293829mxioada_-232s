function ModuleFunction()

  for i,v in pairs(game.Players.LocalPlayer:GetDescendants()) do -- looping through localplayer
    if v.ClassName == "ModuleScript" then -- checking if v is a module script

      if  v.Name == "PlayerModule" or  v.Parent.Name == "PlayerModule" or  v.Name == "ChatScript" or  v.Name == "ChatMain" or  v.Parent.Name == "ChatMain" or  v.Name == "ControlModule" or  v.Parent.Name == "ControlModule" or  v.Name == "CameraModule" or  v.Parent.Name == "CameraModule" or  v.Name == "Popper" then
      else -- ignore ok





        ModuleContainer.CanvasSize = ModuleContainer.CanvasSize + UDim2.new(0,0,0.2,0) -- making scrollling frame longer ha

        -- gui creation

        local ModuleInfo = Instance.new("Frame", ModuleContainer)
        local ModuleIcon_xd = Instance.new("ImageLabel")
        local ModuleName = Instance.new("TextLabel")
        local ModuleInfoBtn = Instance.new("TextButton")




        ModuleInfo.Name = "ModuleInfo"
        ModuleInfo.BackgroundColor3 = Color3.fromRGB(58, 58, 58)
        ModuleInfo.BorderSizePixel = 0
        ModuleInfo.Size = UDim2.new(0, 395, 0, 36)

        ModuleIcon_xd.Name = "ModuleIcon_xd"
        ModuleIcon_xd.Parent = ModuleInfo
        ModuleIcon_xd.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        ModuleIcon_xd.BackgroundTransparency = 1.000
        ModuleIcon_xd.Size = UDim2.new(0, 36, 0, 36)
        ModuleIcon_xd.Image = "rbxassetid://4800245565"

        ModuleName.Name = "ModuleName"
        ModuleName.Parent = ModuleInfo
        ModuleName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        ModuleName.BackgroundTransparency = 1.000
        ModuleName.Position = UDim2.new(0.106329113, 0, 0, 0)
        ModuleName.Size = UDim2.new(0, 353, 0, 36)
        ModuleName.Font = Enum.Font.SourceSans
        ModuleName.Text = v.Name
        ModuleName.TextColor3 = Color3.fromRGB(255, 255, 255)
        ModuleName.TextSize = 18.000
        ModuleName.TextXAlignment = Enum.TextXAlignment.Left

        ModuleInfoBtn.Name = "ModuleInfoBtn"
        ModuleInfoBtn.Parent = ModuleInfo
        ModuleInfoBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        ModuleInfoBtn.BackgroundTransparency = 1.000
        ModuleInfoBtn.Size = UDim2.new(0, 395, 0, 36)
        ModuleInfoBtn.Visible = true
        ModuleInfoBtn.Text = ""
        ModuleInfoBtn.Font = Enum.Font.SourceSans
        ModuleInfoBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
        ModuleInfoBtn.TextSize = 14.000


			

        ModuleInfoBtn.MouseButton1Click:connect(function()

					        for i2,v2 in pairs(require(v)) do
						modulerequire = require(v)


ModuleContainer.Visible = false

						          local ModuleInfoMain = Instance.new("ImageLabel", ModuleValueContainer)
						 local OpenOptionsBtn = Instance.new("TextButton")

          local Icon69 = Instance.new("ImageLabel")
          local GenerateModuleScriptBtn = Instance.new("TextButton")
          local TableMain = Instance.new("Frame")
          local TableNameIcon = Instance.new("ImageLabel")
          local TableValueText = Instance.new("TextLabel")
          local ValueNameIcon = Instance.new("ImageLabel")
          local TableValueBox = Instance.new("TextBox")

         



          OpenOptionsBtn.Name = "OpenOptionsBtn"
          OpenOptionsBtn.Parent = ModuleInfoMain
          OpenOptionsBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
          OpenOptionsBtn.BackgroundTransparency = 1.000
          OpenOptionsBtn.Position = UDim2.new(0.149456516, 0, 0.337837845, 0)
          OpenOptionsBtn.Size = UDim2.new(0, 309, 0, 39)
          OpenOptionsBtn.Font = Enum.Font.SourceSans
          OpenOptionsBtn.Text = ""
          OpenOptionsBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
						          OpenOptionsBtn.TextSize = 14.000
						
						          OpenOptionsBtn.MouseButton2Click:connect(function()
							
							 Tabval = TableValueText.Text
							Tabbox = TableValueBox.Text
							print(Tabval)
							local plr = game.Players.LocalPlayer

local mouse = plr:GetMouse()

							
							RightModule.Visible = true
							RightModule.Position = UDim2.new(0, mouse.X, 0, mouse.Y)
							
							
						
						
						          end)
						
						

						
						GenerateModuleScriptBtn.MouseButton1Click:connect(function() -- script generation

setclipboard("-- This script was generated with clvcore\n\ngame." .. v:GetFullName())

end)
          -- change table element for modules

						
						
						
						
						
						 ExplorerBtn.MouseButton1Click:connect(function()
							RightModule.Visible = false
							_G.ClvcoreExplorerAdd(Tabval, Tabbox)
							return
							
						end)
						
						
						MaeScriptBtn.MouseButton1Click:connect(function()
							RightModule.Visible = false
							
							setclipboard("-- This script was generated with clvcore\n\noh_require = require\noh_path = game." .. v:GetFullName() .. "\noh_old_value = " .. Tabval .. "\noh_new_value = ChangeThisToWhatYouWant -- replace this with the new value you want it to change to\n\noh_PathRequired = oh_require(oh_path)\n\nfor _,v in pairs(oh_PathRequired) do\noh_PathRequired." .. Tabval .. " = oh_new_value\nend\n\n-- WARNING: SCRIPT MAY NOT WORK AS INTENDED, DO NOT RELY ON THIS FOR 100% FUNCTIONAL SCRIPTS")
	
					
						end)
						
						
						
						ChangeValueBtn.MouseButton1Click:connect(function()
							
							RightModule.Visible = false
								
							

          ChangeModuleValueTab.Visible = true
          TextBox.Text = Tabbox
          ElementValueText.Text = Tabval

          if tonumber(TextBox.Text) then
            ElementText.Text = "number"
          elseif TextBox.Text == "false" or TextBox.Text == "true" then
            ElementText.Text = "boolean"
          else
            ElementText.Text = "string"
          end

          end)

          local numberDropDown = false
          local stringDropDown = false
          local booleanDropDown  = false

          if ElementText.Text == "number" then

            numberDropDown = true
            booleanDropDown = false
            stringDropDown = false
          elseif ElementText.Text == "boolean" then

            numberDropDown = false
            stringDropDown = false
            booleanDropDown = true
          else

            stringDropDown = true
            numberDropDown = false
            booleanDropDown  = false
          end

          DropDownBtn.MouseButton1Click:connect(function()
          DropDownOptions.Visible = true
          end)



          NumberElementBtn.MouseButton1Click:connect(function()
          numberDropDown = true
          booleanDropDown = false
          stringDropDown = false
          ElementText.Text = "number"
          DropDownOptions.Visible = false
          end)

          StringElementBtn.MouseButton1Click:connect(function()
          numberDropDown = false
          booleanDropDown = false
          stringDropDown = true
          ElementText.Text = "string"
          DropDownOptions.Visible = false
          end)

          BooleanElementBtn.MouseButton1Click:connect(function()
          numberDropDown = false
          booleanDropDown = true
          stringDropDown = false
          ElementText.Text = "boolean"
							          DropDownOptions.Visible = false
							
						end)
						
						
						
						

          TextBox.FocusLost:connect(function(enter)
          if enter then


            if ElementText.Text == "number" == true then

              if tonumber(TextBox.Text) then
                local val = TextBox.Text
                pcall(function() loadstring(tostring("local oh = require(game." .. v:GetFullName() .. ")\noh." .. Tabval .. " = " .. TextBox.Text))() end);
                ChangeModuleValueTab.Visible = false
                for i,v in pairs(ModuleValueContainer:GetDescendants()) do
                  if v.Name == "ModuleInfoMain" then
												                    if v.ClassName == "TextLabel" then
													if v.Text == Tabval then
                      v.Parent.Parent.ValueNameIcon.TableValueBox.Text = TextBox.Text
                    end
                  end
											                end
											end
                TextBox.Text = val
                ChangeModuleValueTab.Visible = false
              else
                MessageBoxInitUI("Integar is not a number!")
                return
              end

            elseif ElementText.Text == "string" == true then

              if tonumber(TextBox.Text) then
                MessageBoxInitUI("Integar is not a string!")
                return
              else
                local val = TextBox.Text
                pcall(function() loadstring(tostring("local oh = require(game." .. v:GetFullName() .. ")\noh." .. Tabval .. " = " .. TextBox.Text))() end);
                ChangeModuleValueTab.Visible = false
                 for i,v in pairs(ModuleTab:GetDescendants()) do
                  if v.Name == "TableValueText" then
                    if v.Text == Tabval then
                      v.Parent.Parent.ValueNameIcon.TableValueBox.Text = TextBox.Text
                    end
                  end
                end

                TextBox.Text = val
                ChangeModuleValueTab.Visible = false


              end


            elseif ElementText.Text == "boolean" == true then

              if TextBox.Text == "true" or TextBox.Text == "false" then
                local val = TextBox.Text
                pcall(function() loadstring(tostring("local oh = require(game." .. v:GetFullName() .. ")\noh." .. Tabval .. " = " .. TextBox.Text))() end);
                ChangeModuleValueTab.Visible = false
                for i,v in pairs(ModuleTab:GetDescendants()) do
                  if v.Name == "TableValueText" then
                    if v.Text == Tabval then
                      v.Parent.Parent.ValueNameIcon.TableValueBox.Text = TextBox.Text
                    end
                  end
                end
                TextBox.Text = val
                ChangeModuleValueTab.Visible = false
              else
                MessageBoxInitUI("Integar is not a boolean!")
                return


									              end
								

            end
          end










          end)



          -- end of elemt ui






          ModuleInfoMain.Name = "ModuleInfoMain"
          ModuleInfoMain.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
          ModuleInfoMain.BackgroundTransparency = 1.000
          ModuleInfoMain.Position = UDim2.new(0.0208877288, 0, 0.0427807495, 0)
          ModuleInfoMain.Size = UDim2.new(0, 368, 0, 74)
          ModuleInfoMain.Image = "rbxassetid://2851926732"
          ModuleInfoMain.ImageColor3 = Color3.fromRGB(58, 58, 58)
          ModuleInfoMain.ScaleType = Enum.ScaleType.Slice
          ModuleInfoMain.SliceCenter = Rect.new(12, 12, 12, 12)

          Icon69.Name = "Icon69"

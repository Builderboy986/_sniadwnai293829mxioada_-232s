function _G.ClvcoreExplorerAdd(Name, Content)
	
	for i,v in pairs (CustomExplorerContainer:GetDescendants()) do
		if v.ClassName == "TextLabel" then
			if v.Text == Name then
				MessageBoxInitUI(Name.. " Already exists in explorer")
				return
			end
		end
	end
	
	local Custom = Instance.new("Frame", CustomExplorerContainer)
local CustomIcon = Instance.new("ImageLabel")
local CustomText = Instance.new("TextLabel")
	local CustomBtn = Instance.new("TextButton")
	

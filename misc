CacheBtn.MouseButton1Click:connect(function()
	local owo, uwu = pcall(function() loadstring(tostring(scripttext.Text))() end);
	if uwu then
		ErrorText.Text = uwu
	end
end)


CopyBtn.MouseButton1Click:connect(function()
	setclipboard(scripttext.Text)
end)

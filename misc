function ButtonScripts()

	  ModuleBtn.MouseButton1Click:connect(function()
		ClearModuleCon()

  if ModuleValueContainer.Visible == true then
			    ClearModuleValCon()
			ModuleContainer.Visible = true
  end

  ModuleTab.Visible = true
  ConstantTab.Visible = false
  ValueTab.Visible = false
  ScriptScanTab.Visible = false
  ExplorerContentsTab.Visible = false
  HttpSpyTab.Visible = false
		  ScriptDebugTab.Visible = false
		UpvalueScannerTab.Visible = false
  ModuleFunction()
  end)


	  ScriptScanBtn.MouseButton1Click:connect(function()
		ScriptConstantContainer.Visible = false
		ClearScriptconsCon()
		ClearScriptFuncCon()
		ScriptFunctionContainer.Visible = false
		Text_3.Visible = true
				Text_2.Visible = true
				Text.Visible = true
		ClearScriptCon()
		ScriptFunction()
  ModuleTab.Visible = false
  ConstantTab.Visible = false
  ValueTab.Visible = false
  ScriptScanTab.Visible = true
  ExplorerContentsTab.Visible = false
  HttpSpyTab.Visible = false
		  ScriptDebugTab.Visible = false
		UpvalueScannerTab.Visible = false
  end)


  HttpSpyBtn.MouseButton1Click:connect(function()
  ModuleTab.Visible = false
  ConstantTab.Visible = false
  ValueTab.Visible = false
  ScriptScanTab.Visible = false
  ExplorerContentsTab.Visible = false
		  HttpSpyTab.Visible = false
		UpvalueScannerTab.Visible = true
  ScriptDebugTab.Visible = false
  end)

	  ValueBtn.MouseButton1Click:connect(function()
		ClearValCon()
		ValueExplorer()
  ModuleTab.Visible = false
  ConstantTab.Visible = false
  ValueTab.Visible = true
  ScriptScanTab.Visible = false
  ExplorerContentsTab.Visible = false
  HttpSpyTab.Visible = false
  ScriptDebugTab.Visible = false
  end)

  ConstantBtn.MouseButton1Click:connect(function()
  ModuleTab.Visible = false
  ConstantTab.Visible = true
  ValueTab.Visible = false
  ScriptScanTab.Visible = false
  ExplorerContentsTab.Visible = false
  HttpSpyTab.Visible = false
		  ScriptDebugTab.Visible = false
		
		UpvalueScannerTab.Visible = false
  end)

	  ExplorerToggleBtn.MouseButton1Click:connect(function()
  ModuleTab.Visible = false
  ConstantTab.Visible = false
  ValueTab.Visible = false
  ScriptScanTab.Visible = false
  ExplorerContentsTab.Visible = true
  HttpSpyTab.Visible = false
		  ScriptDebugTab.Visible = false
		UpvalueScannerTab.Visible = false
  end)

  ScriptDebugBtn.MouseButton1Click:connect(function()
  ModuleTab.Visible = false
  ConstantTab.Visible = false
  ValueTab.Visible = false
  ScriptScanTab.Visible = false
  ExplorerContentsTab.Visible = false
  HttpSpyTab.Visible = false
		  ScriptDebugTab.Visible = true
		UpvalueScannerTab.Visible = false
  end)

end

function _G.ClvcoreDeleteAll()
	for i,v in pairs(CustomExplorerContainer:GetDescendants()) do
		if v.ClassName == "Frame" then
			v:Destroy()
		end
	end
end

function _G.ClvcoreDelete(Name, Content)
	for i,v in pairs(CustomExplorerContainer:GetDescendants()) do
		if v.ClassName == "TextLabel" then
			if v.Text == Name then
				
				
				for a,b in pairs(ExplorerContentsContainer:GetDescendants()) do
					if b.ClassName == "TextBox" then
						if b.Text == Name then
							b:Destroy()
						end
					end
				end
				
				v.Parent:Destroy()
			end
		end
	end
	CustomExplorerContainer.CanvasSize = CustomExplorerContainer.CanvasSize - UDim2.new(0,0,0.2,0)
	if CustomExplorerContainer:GetChildren() == nil then
		CustomExplorerContainer.CanvasSize = UDim2.new(0,0,0,0)
	end

	
end

